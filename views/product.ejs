<%- include('header'); -%>

    <!-- header -->
    <header>
        <div class="headercontainer rounded container d-flex flex-column justify-content-center">
            <img class="img-fluid align-self-center" src="images\title.png" alt="titlelogo" width="500"
                height="auto"></a>
            <div class="headerdescription align-self-center text-center">Melbourne's first and cuteset eyewear accessory
                fashion brand.</div>
            <div class="est align-self-center pt-2">EST.2024</div>
        </div>
        </div>
    </header>
    <!-- end header -->
    <main>
        <div class="container">
            <div class="card card-body border-0">
                <div class="row row-cols-1 row-cols-md-2 g-4">

                    <% rows.forEach(function(row) { %>
                        <div class="col">
                            <div class="card border-0">
                                <img src="images/<%= row.productimage %>" class="card-img-top" alt="case">
                                <div class="card-body text-center">
                                    <h5 class="card-title best-selling-cases">
                                        <%= row.productname %>
                                    </h5>
                                    <p class="card-text best-selling-price">$<%= row.productprice %></p>
                                    <button class="bestsellercartbutton">
                                        <img src="images/cart.png" width="30" height="auto">
                                    </button>
                                </div>
                            </div>
                        </div>
                    <% }); %>
                </div>
            </div>
        </div>
    </main>




    <%- include('footer'); -%>